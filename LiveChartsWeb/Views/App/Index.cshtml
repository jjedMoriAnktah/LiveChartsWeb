<!DOCTYPE html>
<html lang="en" ng-app="liveChartsApp">

<head>
    <title>Live Charts</title>

    <base href="/App/">

    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="description" content=""/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    <script src="https://use.typekit.net/tkh3uwr.js"></script>
    <script>
        try {
            Typekit.load({ async: true });
        } catch (e) {
        }
    </script>

    @Styles.Render("~/AppContent/css")

    <style type="text/css">
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }
    </style>
</head>

<body ng-cloak>

<nav class="navbar navbar-default navbar-fixed-top" role="navigation" ng-controller="templateController">
    <div id="menu" class="container" ng-class="{ 'is-closed': !isVisible }">
        <div class="navbar-header">
            <a class="navbar-brand" href="@Url.Action("Index", "Home")" style="padding-top: 8px">
                <img alt="Controli" src="~/Content/Images/Logos/LiveChartsLogo30.png"/>
            </a>
            <button type="button" class="navbar-toggle collapsed" ng-click="toggleMenu()">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div>
            <ul class="nav navbar-nav" ng-click="hideMenu()">
                <li><a href="/App/documentation/introduction">Documentation</a></li>
                <li><a href="/App/examples/intro?path=intro">Get Started</a></li>
            </ul>
        </div>
    </div>
</nav>

<a href="https://github.com/beto-rodriguez/Live-Charts" class="hidden-xs">
    <img style="position: fixed; top: 0; right: 0; border: 0; z-index: 10000" src="https://camo.githubusercontent.com/52760788cde945287fbb584134c4cbc2bc36f904/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f77686974655f6666666666662e706e67" alt="Fork me on GitHub" data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_white_ffffff.png">
</a>

<div ng-view class="view">

</div>

<div class="container-fluid">
    <div class="row" style="margin-top: 25px;">
        <div class="col-sm-offset-3 col-xs-9 text-center">
            <p class="text-muted">
                Live Charts is an open source project maintained by <a href="https://github.com/beto-rodriguez">Beto Rodríguez</a> and the github community, get involved and contribute
            </p>
            <p class="text-muted">
                Icons made by
                <a href="http://www.flaticon.com/authors/madebyoliver" title="Madebyoliver">Madebyoliver</a> and
                <a href="http://www.freepik.com" title="Freepik">Freepik</a>
                from <a href="http://www.flaticon.com" title="Flaticon">www.flaticon.com</a>
                is licensed by <a href="http://creativecommons.org/licenses/by/3.0/" title="Creative Commons BY 3.0" target="_blank">CC 3.0 BY</a><br/>
            </p>
            <p>
                <a href="/Home/PrivacyPolicy">Privacy Policy</a>
            </p>
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/app")

<script>
    ((window.gitter = {}).chat = {}).options = {
        room: 'beto-rodriguez/Live-Charts'
    };
</script>

<script src="https://sidecar.gitter.im/dist/sidecar.v1.js" async defer></script>

